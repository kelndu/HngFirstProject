        <section id="contactForm">
            <h2>Contact</h2>
            <form action="/contact" method="post">
              <div class="form-group">
                <label for="name">Name</label>
                <input type="text" name="name" required="" minlength="3" maxlength="30" id="name" class="form-control" placeholder="Name" aria-describedby="nameId">
                <small id="nameHelp" class="text-muted">eg: John Doe</small>
              </div>
              <div class="form-group">
                <label for="email">Email</label>
                <input type="email" name="email" required="" id="email" class="form-control" placeholder="Email Address" aria-describedby="emailId">
                <small id="emailId" class="text-muted">eg: johndoe@gmail.com</small>
              </div>
              <div class="form-group">
                <label for="subject">Subject</label>
                <input type="text" name="subject" id="subject" class="form-control" required="" minlength="3" maxlength="30" placeholder="Subject" aria-describedby="subjectHelp">
                <small id="subjectHelp" class="text-muted">eg: Job Hire</small>
              </div>
              <div class="form-group">
                <label for="message">Message</label>
                <textarea class="form-control" name="message" id="message" required="" minlength="3" maxlength="200" rows="3"></textarea>
              </div>
              <button type="submit" id="submitBtn" class="btn btn-primary">Submit</button>
            </form>
          </section>
